<!DOCTYPE html>
<html lang="en">
    <head>
        <meta charset="UTF-8">
        <meta name="viewport" content="width=device-width, initial-scale=1.0">
        <meta http-equiv="X-UA-Compatible" content="ie=edge">
        <title>Users</title>
    </head>
    <body>
        <h1>Here are some Users!</h1>
        @model List<string>

            @{
                foreach(string name in Model)
                {
                    <p>@name</p>
                }
            }
        
    </body>
</html>
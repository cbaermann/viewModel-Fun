<!DOCTYPE html>
<html lang="en">
    <head>
        <meta charset="UTF-8">
        <meta name="viewport" content="width=device-width, initial-scale=1.0">
        <meta http-equiv="X-UA-Compatible" content="ie=edge">
        <title>Numbers</title>
    </head>

    <body>
        <h1>Here are some numbers!</h1>
        @model int[]
        @{
            foreach(int number in Model)
            {
                <p>@number</p>
            }
        }
        
    </body>
</html>